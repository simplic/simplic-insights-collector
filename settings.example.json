{
    "$schema": "./schemas/settings.schema.json",
    "uuid": "01234567-89ab-cdef-0123-456789abcdef",
    "name": "<name>",
    "url": "<url>",
    "token": "<access token>",
    "upload": {
        "min_interval": "10s",
        "max_interval": "5m",
        "max_backlog": 50
    },
    "sensors": [
        {
            "uuid": "01234567-89ab-cdef-0123-456789abcdef",
            "type": "system:cpu",
            "name": "CPU Usage",
            "interval": "2s",
            "settings": {
                "unhealthy": "percent > 80",
                "degraded": "percent > 70",
                "extra": false
            }
        },
        {
            "uuid": "01234567-89ab-cdef-0123-456789abcdef",
            "type": "system:ram",
            "name": "RAM Usage",
            "interval": "2s",
            "settings": {
                "unhealthy": "mem.percent > 80",
                "degraded": "mem.percent > 70",
                "extra": false
            }
        }
    ]
}